{
  "name": "dhl-tracking-proxy",
  "version": "1.0.0",
  "description": "Proxy server for DHL tracking that uses web scraping",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "postinstall": "node install-chrome.js",
    "download-chrome": "npx -y @puppeteer/browsers install chrome@stable"
  },
  "puppeteer": {
    "skipChromiumDownload": false
  },
  "keywords": ["dhl", "tracking", "proxy", "scraping"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "puppeteer": "^21.5.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "@puppeteer/browsers": "^1.7.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}


